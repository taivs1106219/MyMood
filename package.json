{
  "name": "mymood",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "start": "webpack --watch --mode=development && electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taivs1106219/MyMood.git"
  },
  "author": "taivs1106219, ryantsui",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/taivs1106219/MyMood/issues"
  },
  "homepage": "https://github.com/taivs1106219/MyMood#readme",
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "classnames": "^2.5.1",
    "css-loader": "^7.1.2",
    "electron": "^32.0.1",
    "electron-builder": "^25.1.7",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "postcss-loader": "^8.1.1",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.6",
    "react-dom": "^18.3.1",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "archiver": "^7.0.1",
    "archiver-utils": "^5.0.2",
    "bootstrap": "^5.3.3",
    "electron-is-packaged": "^1.0.2",
    "openai": "^4.72.0",
    "tar": "^7.4.3",
    "twilio": "^5.3.6"
  },
  "build": {
    "appId": "io.github.taivs1106219.MyMood",
    "directories": {
      "output": "out"
    },
    "generateUpdatesFilesForAllChannels": true,
    "productName": "MyMood",
    "files": [
      "main.js",
      "package.json",
      "preload.js",
      "default_configs.js",
      "res",
      "src/js/getDateNum.js",
      "dist"
    ],
    "nsis": {
      "oneClick": false
    },
    "win": {
      "artifactName": "${productName}-${os}-${arch}.${ext}",
      "icon": "./res/images/icon.png",
      "extraFiles": [
        "platform-tools-win"
      ],
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "publish": [
        {
          "provider": "github",
          "owner": "platron-pt",
          "repo": "platron"
        }
      ]
    },
    "linux": {
      "extraFiles": "platform-tools-linux",
      "artifactName": "${productName}-${os}-${arch}.${ext}",
      "target": [
        {
          "target": "tar.xz",
          "arch": "x64"
        }
      ],
      "publish": [
        "github"
      ]
    }
  }
}
